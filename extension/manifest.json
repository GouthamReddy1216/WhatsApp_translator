{
    "manifest_version": 3,
    "name": "WhatsApp Translator",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_title": "Translate WhatsApp Messages"
    },
    "content_scripts": 
    [
      {
        "js": [
          "contentscript.js"
        ],
        "matches": [
          "https://*.whatsapp.com/"
        ]
      }
    ]
    ,
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ]
  }
  